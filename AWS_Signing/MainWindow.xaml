<Window x:Class="AWSSigning.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AWS_Signing"
        mc:Ignorable="d"
        Title="AWS Sign" Height="192" Width="402" WindowStartupLocation="CenterScreen">
    <Grid Background="#FFE2DD2D">
        <Button Name="signButton" IsDefault="True" Content="Sign in" HorizontalAlignment="Left" Margin="157,93,0,0" VerticalAlignment="Top" Width="120" Height="26" Click="signButton_Click"/>
        <TextBox Name="tokenBox" HorizontalAlignment="Left" Margin="157,50,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="23"/>
        <Label Content="Your mfa code" HorizontalAlignment="Left" Margin="56,50,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
